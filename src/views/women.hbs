
<section>
<!-- Slideshow container -->
{{#each result}}
<div class="slideshow-container">
<!-- Full-width images with number and caption text -->
<div class="mySlides fade">
  <div class="discount">
    <p class="discountText">discount</p>
  </div>
  <div class="numbertext">1 / 2</div>
  <a href="/details/{{this.id}}">
  <img src="{{this.img}}" style="width:100%" class='sliderImg'>
</a>
  <div class="text"></div>
</div>

<!-- Next and previous buttons -->
<a class="prev" onclick="plusSlides(-1)">&#10094;</a>
<a class="next" onclick="plusSlides(1)">&#10095;</a>
</div>
{{/each}}
<br>
<!-- The dots/circles -->
</div>
<div class="dot-container" style="display:flex;flex-direction:row;justify-content:center;margin-top:600px">
  {{#each result}}
    <div style="text-align:center">
      <span class="dot" onclick="currentSlide({{@index}})"></span>
    </div>
  {{/each}}
</div>
</section>

<section>
