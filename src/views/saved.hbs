<!DOCTYPE html>
<html>

<body>

<div class="container">
<div class="boxs">
  {{#each savedfashion}}
<div class="box">
  <div class="innerbox">
    <a class="a" href="#"><img src="{{this.img}}" alt="fashion"></a>
  </div>
  <p class="info">{{this.price}} NIS <a><i id="{{this.id}}" class="fa fa-heart" onclick="savedpage({{this.id}})"></i></a></p>
  <div id="myModal" class="modal">
    <div class="modal-content">
      <span id="close" class="close">&times;</span>
      <p id="text"></p>
    </div>
  </div>
  <p>{{this.name}}</p>
</div>
  {{/each}}
</div>
</div>

<script src="/js/savedFashion.js"></script>
</body>

</html>
